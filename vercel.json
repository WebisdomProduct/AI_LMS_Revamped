{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/server.cjs"
        }
    ],
    "builds": [
        {
            "src": "server/server.cjs",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static"
        }
    ]
}